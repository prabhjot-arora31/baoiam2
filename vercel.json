{
  "version": 2,
  "builds": [
    {
      "src": "baoiamweb/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "buildCommand": "python manage.py migrate && python manage.py collectstatic --noinput",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "(.*)",
      "dest": "baoiamweb/wsgi.py"
    }
  ]
}
